        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizSpark">
        <activity
            android:name=".CelebrityQuiz.CelebrityQuestionsActivity"
            android:exported="false" />
        <activity
            android:name=".HistoryQuiz.HistoryQuizActivity"
            android:exported="false" />
        <activity
            android:name=".HistoryQuiz.QuizResultActivity"
            android:exported="false" />
        <activity
            android:name=".MainScreen.MainScreenActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".MainScreen.StartGameActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity">
